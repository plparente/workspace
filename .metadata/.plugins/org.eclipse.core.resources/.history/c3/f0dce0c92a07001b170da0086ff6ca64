package br.com.excecao.teste;

public class TesteExcecao {

	public static void main(String[] args) {
		try {
		int numero = Integer.parseInt("A");
		System.out.println(numero);
		}catch(Exception e) {
			System.out.println("Digite um número");
		}
		
		
	} //	fechando o método main()

	
	
	
	
	
	
} // fechando a classe
