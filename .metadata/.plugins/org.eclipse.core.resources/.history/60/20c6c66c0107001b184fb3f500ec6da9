package br.com.universidade.modelo;

public class Turma extends Formacao {

	private String nome;
	private String periodo;
	private Curso curso;
	private Aluno aluno;
	private Professor professor;
	
	
	
	
	public String getAll() {
		return 
				super.getAll() + "\n" +
 				"Projeto Nome: " + nome + "\n" + 
 				"Projeto Período: " + periodo + "\n" +
 				"Projeto Curso: " + curso + "\n" +
 				"Projeto Aluno: " + aluno + "\n" +
				"Carga Pofessor: " + professor; 
	}
	
	
	public Turma() {
		super();
	}
	public Turma(float valor, String descricao, int duracao, String nome, String periodo, Curso curso, Aluno aluno,
			Professor professor) {
		super(valor, descricao, duracao);
		this.nome = nome;
		this.periodo = periodo;
		this.curso = curso;
		this.aluno = aluno;
		this.professor = professor;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getPeriodo() {
		return periodo;
	}
	public void setPeriodo(String periodo) {
		this.periodo = periodo;
	}
	public Curso getCurso() {
		return curso;
	}
	public void setCurso(Curso curso) {
		this.curso = curso;
	}
	public Aluno getAluno() {
		return aluno;
	}
	public void setAluno(Aluno aluno) {
		this.aluno = aluno;
	}
	public Professor getProfessor() {
		return professor;
	}
	public void setProfessor(Professor professor) {
		this.professor = professor;
	}
	
	
	
}
