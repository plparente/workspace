package br.com.universidade.modelo;

public class Medio extends Formacao {

	private int tipo;

	
	public void setAllMedio(float valor, String descricao, int duracao, int tipo) {
		super.setAll(valor, descricao, duracao);
		this.tipo = tipo;
	

	public String getAll() {
		return 
				super.getAll() + "\n" +
 				"Projeto Médio: " + medio; 
 							
}
	
	
	public Medio() {
		super();
	}

	public Medio(float valor, String descricao, int duracao, int tipo) {
		super(valor, descricao, duracao);
		this.tipo = tipo;
	}

	public int getTipo() {
		return tipo;
	}

	public void setTipo(int tipo) {
		this.tipo = tipo;
	}
	
	
	
}
